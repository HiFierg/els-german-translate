# d:/renpy-6.99.12.4/scenario/epilogue_uv.rpyc:2110
translate german epilogue_uv_ulya_b558a3fc:

    # uv "Ну, как бы тебе сказать…"
    #uv "Well, how should I put it..."
    uv "Nun, wie soll ich sagen…"

