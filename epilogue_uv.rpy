# d:/renpy-6.99.12.4/scenario/epilogue_uv.rpyc:2056
translate german epilogue_uv_ulya_6656d4b0:

    # me "Но я ведь не помню…"
    #me "But I don't remember..."
    me "Aber ich kann mich nicht erinnern…"

