translate german epilogue_mi_64fd49e3:

    # ma "А какая разница…"
    #ma "Who cares..."
    ma "Wen interessierts.…"

# d:/renpy-6.99.12.4/scenario/epilogue_mi.rpyc:836
translate german epilogue_mi_9eb4aadf:

    # my "Ну… Раз так…"
    #my "Well... In that case..."
    my "Nun… Na dann…"

# d:/renpy-6.99.12.4/scenario/epilogue_mi.rpyc:838
translate german epilogue_mi_a20cefa7_7:

    # "..."
    "..."

# d:/renpy-6.99.12.4/scenario/epilogue_mi.rpyc:852
